<div class="field category multiselect">
  <%= f.attribute_fields do |a| %>
    <%= a.attribute_select associations: [:category] %>
  <% end %>
  <%= f.predicate_select only: %i(cont eq) %>
  <%= f.value_fields do |v| %>
    <%= v.text_field :value %>
  <% end %>
  <%= f.submit "Search" %>
  <%= link_to "remove", '#', class: "remove_fields" %>
</div>