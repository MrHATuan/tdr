<div class="row" id="review_<%review.id%>" >
  <div class="col-md-9">
    <div class="col-md-11">
      <b>
        <%= link_to review.user.name, review.user %>
      </b>
      <%= review.content %>
      <br/>
      <h6>
        <%= review.created_at.strftime t(:timetype) %>
      </h6>
    </div>
  </div>
</div>
